{
  "extends": ["airbnb", "next/core-web-vitals", "prettier"],
  "parser": "@typescript-eslint/parser",
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parserOptions": {
        "project": ["./tsconfig.json"]
      }
    }
  ],
  "rules":  {
    "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx"] }]
  },
  "plugins": ["prettier"]
}
